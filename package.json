{
  "name": "caee-page-build-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:test": "vite --mode staging",
    "dev:prod": "vite --mode prod",
    "build": "npm run lint && vite build --mode prod",
    "build:test": "npm run lint && vite build --mode staging",
    "preview": "vite preview --port 8080",
    "lint": "eslint src/ --ext .ts,.vue,.tsx && vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "element-plus": "^1.2.0-beta.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "eslint": "^8.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.43.4",
    "typescript": "^4.4.3",
    "unplugin-element-plus": "^0.1.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
